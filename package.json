{
  "name": "cypress-real-world-app-wdio",
  "description": "Cypress-Real-World-App -- testing using WebDriverIO",
  "type": "module",
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@wdio/allure-reporter": "^8.24.12",
    "@wdio/cli": "^8.26.1",
    "@wdio/local-runner": "^8.26.1",
    "@wdio/mocha-framework": "^8.24.12",
    "@wdio/spec-reporter": "^8.24.12",
    "allure-commandline": "^2.25.0",
    "cross-env": "^7.0.3"
  },
  "scripts": {
    "wdio:chrome": "cross-env ENV='dev' wdio run ./config/wdio.config.chrome.js",
    "wdio:firefox": "cross-env ENV='dev' wdio run ./config/wdio.config.firefox.js",
    "wdio:edge": "cross-env ENV='dev' wdio run ./config/wdio.config.edge.js",
    "report": "allure open allure-report",
    "generate": "npx allure generate allure-results --clean"
  },
  "keywords": [
    "wdio",
    "cypress-real-world-app",
    "automation",
    "javascript",
    "allure"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kristinap8/cypress-real-world-app-wdio.git"
  }
}
